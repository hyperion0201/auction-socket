<Window x:Class="Client.AuctionResult"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="AuctionResult" Height="450" Width="800">
    <Canvas>
        <TextBlock Text="Congratulation! Auction success!" FontSize="20" Canvas.Left="234" Canvas.Top="34"/>
        <TextBlock Text="Fill this infomation to complete: " FontSize="20" Canvas.Left="147" Canvas.Top="100"/>
        <TextBlock Text="Bank ID" FontSize="20" Canvas.Left="147" Canvas.Top="153"/>
        <TextBox Name="bankIDTextbox" Width="300" Height="35" Canvas.Left="147" Canvas.Top="185"/>
        <TextBlock Text="Card ID" FontSize="20" RenderTransformOrigin="2.544,6.789" Canvas.Left="147" Canvas.Top="240"/>
        <TextBox Name="cardIDTextbox" Width="300" Height="35" Canvas.Left="147" Canvas.Top="267"/>
        <Button Name="finishButton" Click="FinishButton_Click" Content="Finish" FontSize="20" Width="100" Height="35" RenderTransformOrigin="2.134,4.709" Canvas.Left="602" Canvas.Top="347"/>
    </Canvas>
</Window>
